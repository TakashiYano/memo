    "@supabase/supabase-js": "^2.26.0",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/typography": "^0.5.9",
    "classcat": "5.0.4",
    "date-fns": "^2.30.0",
    "next": "^13.5.2",
    "next-themes": "0.2.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hot-toast": "^2.4.0",
    "react-textarea-autosize": "^8.4.0",
    "use-debounce": "^9.0.4"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^4.1.0",
    "@types/node": "18.11.18",
